@model IEnumerable<ItTutorial.Models.QuizPergunta>

        @foreach (var item in Model)
        {
            
             @Html.Partial("Quizparcial", item)

        }
 
<input type="submit" onclick="saveState()" value="SUBMIT" />

<script>
    function saveState() {
        var ans1 = document.getElementById('Radio1');
        if (ans1.value == 1) {
            ans1.setAttribute("checked", "checked");
            ans1.checked = true;
        } 
    } 
</script>